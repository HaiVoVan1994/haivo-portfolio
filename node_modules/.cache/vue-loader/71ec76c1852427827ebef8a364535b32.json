   <v-list-item-content>\n            <v-list-item-title class=\"subtitile-1\">{{\n              text\n            }}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar\n      app\n      :flat=\"flat\"\n      class=\"px-15\"\n      :class=\"{ expand: flat }\"\n    >\n      <v-toolbar-title>\n          <toggleTheme />\n          <v-btn left text @click=\"$vuetify.goTo('#home')\">\n            <span class=\"mr-2\">HaiVo</span>\n          </v-btn>\n      </v-toolbar-title>\n      <v-spacer />\n      <v-app-bar-nav-icon\n        @click.stop=\"drawer = !drawer\"\n        v-if=\"isXs\"\n      />\n      <div v-else>\n        <v-btn text @click=\"$vuetify.goTo('#experience')\">\n          <span class=\"mr-2\">Experience</span>\n        </v-btn>\n         <v-btn text @click=\"$vuetify.goTo('#skills')\">\n          <span class=\"mr-2\">Skills</span>\n        </v-btn>\n         <v-btn text @click=\"$vuetify.goTo('#projects')\">\n          <span class=\"mr-2\">Project</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#blog')\">\n          <span class=\"mr-2\">Blog</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#contact')\">\n          <span class=\"mr-2\">Contact</span>\n        </v-btn>\n      </div>\n    </v-app-bar>\n  </div>\n</template>\n\n<style scoped>\n.nar-bar-container {\n  height: 80px;\n}\n\n.v-toolbar {\n  transition: 2s;\n}\n\n.expand {\n  height: 80px !important;\n  padding-top: 10px;\n}\n\n.theme--light.v-app-bar.v-toolbar.v-sheet {\n  background-color: #FFFFFF;\n}\n\n.theme--dark.v-app-bar.v-toolbar.v-sheet {\n  background-color: #121212;\n}\n\n</style>\n\n<script>\nimport toggleTheme from \"../components/ToggleTheme.vue\"\n\nexport default {\n  components: {\n    toggleTheme\n  },\n\n  props: {\n    flat: Boolean,\n  },\n\n  data: () => ({\n    drawer: null,\n    isXs: false,\n    items: [\n      [\"fas fa-home\", \"Home\", \"#home\"],\n      [\"fas fa-bookmark\", \"Experience\", \"#experience\"],\n      [\"fas fa-cog\", \"Skills\", \"#skills\"],\n      [\"fas fa-code-branch\", \"Projects\", \"#projects\"],\n      [\"fas fa-blog\", \"Blog\", \"#blog\"],\n      [\"fas fa-envelope\", \"Contact\", \"#contact\"],\n    ],\n  }),\n  \n  mounted() {\n    this.onResize();\n      window.addEventListener(\"resize\", this.onResize, { passive: true });\n  },\n  \n  methods: {\n    onResize() {\n      this.isXs = window.innerWidth < 850;\n    },\n  },\n\n  watch: {\n    isXs(value) {\n      if (!value) {\n        if (this.drawer) {\n          this.drawer = false;\n        }\n      }\n    },\n  }\n};\n</script>\n"]}]}